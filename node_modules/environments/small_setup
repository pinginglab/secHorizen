# setup env
sudo cp local_gae /usr/local/bin
sudo cp gpush /usr/local/bin
sudo cp memfree /usr/local/bin
sudo cp gut /usr/local/bin
sudo cp logoff /usr/local/bin
sudo cp logon /usr/local/bin
sudo cp arx /usr/local/bin
sudo cp ngrok /usr/local/bin
cp .bashrc ~/
cp .vimrc ~/
cp .gitconfig ~/
sudo cp vimrc /etc/vim/vimrc
